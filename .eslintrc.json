{
  "extends": "semistandard",
  "rules": {
    "space-before-function-paren": [
      "error", {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "array-bracket-spacing": "off",
    "quote-props": "off",
    "space-infix-ops": "off",
    "no-loss-of-precision": "off",
    "no-prototype-builtins": "off",
    "brace-style": "off",
    "keyword-spacing": "off",
    "object-curly-spacing": "off"
  }
}
